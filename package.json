{
  "name": "tablo",
  "description": "New tab web extension",
  "version": "1.0.0",
  "scripts": {
    "node:ng": "node --max_old_space_size=8096 ./node_modules/@angular/cli/bin/ng",
    "watch": "ng build --watch",
    "watch:chrome": " ng build --watch --configuration=chrome",
    "watch:web-ext": "ng build --watch --configuration=web-ext",
    "build": "ng build --prod --output-hashing=none",
    "build:chrome": "ng build --prod --output-hashing=none --configuration=chrome",
    "build:web-ext": "ng build --prod --output-hashing=none --configuration=web-ext",
    "test": "ng test",
    "serve": "npm run node:ng -- serve",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.2.0",
    "@angular/common": "~10.2.0",
    "@angular/compiler": "~10.2.0",
    "@angular/core": "~10.2.0",
    "@angular/forms": "~10.2.0",
    "@angular/platform-browser": "~10.2.0",
    "@angular/platform-browser-dynamic": "~10.2.0",
    "@angular/router": "~10.2.0",
    "angular-feather": "^6.0.10",
    "primeicons": "^4.0.0",
    "primeng": "^10.0.0",
    "rxjs": "~6.5.4",
    "tslib": "^2.0.0",
    "uuid": "^8.3.0",
    "zone.js": "~0.10.2",
    "zooming": "^2.1.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1002.0",
    "@angular/cli": "~10.2.0",
    "@angular/compiler-cli": "~10.2.0",
    "@angular/localize": "^10.2.0",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.3"
  }
}
